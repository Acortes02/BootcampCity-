//Distance mouse

void setup ()
{
size(300,200);

}
 void draw ()
{
 background (255);
 float x = width/2;
 float y = height/2;
 
 if (dist(x,y,mouseX,mouseY) < 40)
 {
    fill(255,100,100);
 }
 else 
 {

  fill(0,0,255);
 }
   
 ellipse(x,y,80,80);
 
 fill(0);
 float distanceTocentre =
 dist(x,y,mouseX,mouseY);
 text (distanceTocentre,x,y);

}
